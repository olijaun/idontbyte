@startuml

actor "User A" as U1
actor "User B" as U2
participant "Service" as S
participant "Repository" as R
participant "Account X" as A

U1 -> S: withdraw()
S -> R: getAccount(X)
S <-- R: X : Account
S -> A: withdraw
S <-- A:

U2 -> S: withdraw()
S -> R: getAccount(Y)
S <-- R: Y : Account
S -> A: withdraw
S <-- A:
U2 <-- S:

S -> R: store(Y)

S -> R: store(X)
S <-- R:
U1 <-- S:

@enduml